<?xml version="1.0" ?>
<spirit:component xmlns:spirit="http://www.spiritconsortium.org/XMLSchema/SPIRIT/1685-2009" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.spiritconsortium.org/XMLSchema/SPIRIT/1685-2009 http://www.spiritconsortium.org/XMLSchema/SPIRIT/1685-2009/index.xsd">
  <spirit:vendor>vendor.com</spirit:vendor>
  <spirit:library>components</spirit:library>
  <spirit:name>TABLE0_Component</spirit:name>
  <spirit:version>1.0</spirit:version>
  <spirit:memoryMaps>
    <spirit:memoryMap>
      <spirit:name>CSR_MemoryMap</spirit:name>
      <spirit:addressBlock>
        <spirit:name>CSR_Block</spirit:name>
        <spirit:baseAddress>0x40000000</spirit:baseAddress>
        <spirit:range>0x1014</spirit:range>
        <spirit:width>32</spirit:width>
        <spirit:register>
          <spirit:name>CTRL2</spirit:name>
          <spirit:addressOffset>0x1000</spirit:addressOffset>
          <spirit:size>32</spirit:size>
          <spirit:field>
            <spirit:name>MODE2</spirit:name>
            <spirit:bitRange>[1:0]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:resets>
              <spirit:reset>
                <spirit:resetTypeRef>HARD</spirit:resetTypeRef>
                <spirit:value>0x1</spirit:value>
              </spirit:reset>
            </spirit:resets>
            <spirit:description>Operation mode</spirit:description>
            <spirit:enumeratedValues>
              <spirit:enumeratedValue>
                <spirit:name>Idle</spirit:name>
                <spirit:value>0</spirit:value>
              </spirit:enumeratedValue>
              <spirit:enumeratedValue>
                <spirit:name>Run</spirit:name>
                <spirit:value>1</spirit:value>
              </spirit:enumeratedValue>
              <spirit:enumeratedValue>
                <spirit:name>Sleep</spirit:name>
                <spirit:value>2</spirit:value>
              </spirit:enumeratedValue>
            </spirit:enumeratedValues>
          </spirit:field>
          <spirit:field>
            <spirit:name>PRIORITY</spirit:name>
            <spirit:bitRange>[3:2]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Priority level</spirit:description>
            <spirit:enumeratedValues>
              <spirit:enumeratedValue>
                <spirit:name>Low</spirit:name>
                <spirit:value>0</spirit:value>
              </spirit:enumeratedValue>
              <spirit:enumeratedValue>
                <spirit:name>Med</spirit:name>
                <spirit:value>1</spirit:value>
              </spirit:enumeratedValue>
              <spirit:enumeratedValue>
                <spirit:name>High</spirit:name>
                <spirit:value>2</spirit:value>
              </spirit:enumeratedValue>
            </spirit:enumeratedValues>
          </spirit:field>
          <spirit:field>
            <spirit:name>ENABLE2</spirit:name>
            <spirit:bitRange>[4:4]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Enable block</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>RESETN</spirit:name>
            <spirit:bitRange>[5:5]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:resets>
              <spirit:reset>
                <spirit:resetTypeRef>HARD</spirit:resetTypeRef>
                <spirit:value>0x1</spirit:value>
              </spirit:reset>
            </spirit:resets>
            <spirit:description>Active-low reset</spirit:description>
            <spirit:volatile>true</spirit:volatile>
          </spirit:field>
        </spirit:register>
        <spirit:register>
          <spirit:name>CFG</spirit:name>
          <spirit:addressOffset>0x1004</spirit:addressOffset>
          <spirit:size>32</spirit:size>
          <spirit:field>
            <spirit:name>CLOCKDIV</spirit:name>
            <spirit:bitRange>[7:0]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:resets>
              <spirit:reset>
                <spirit:resetTypeRef>HARD</spirit:resetTypeRef>
                <spirit:value>0x2</spirit:value>
              </spirit:reset>
            </spirit:resets>
            <spirit:description>Clock divider</spirit:description>
          </spirit:field>
        </spirit:register>
        <spirit:register>
          <spirit:name>STAT2</spirit:name>
          <spirit:addressOffset>0x1008</spirit:addressOffset>
          <spirit:size>32</spirit:size>
          <spirit:field>
            <spirit:name>ERR</spirit:name>
            <spirit:bitRange>[0:0]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Error flag</spirit:description>
            <spirit:volatile>true</spirit:volatile>
          </spirit:field>
          <spirit:field>
            <spirit:name>READY</spirit:name>
            <spirit:bitRange>[1:1]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:resets>
              <spirit:reset>
                <spirit:resetTypeRef>HARD</spirit:resetTypeRef>
                <spirit:value>0x1</spirit:value>
              </spirit:reset>
            </spirit:resets>
            <spirit:description>Ready status</spirit:description>
            <spirit:volatile>true</spirit:volatile>
          </spirit:field>
        </spirit:register>
        <spirit:register>
          <spirit:name>DATA2</spirit:name>
          <spirit:addressOffset>0x1010</spirit:addressOffset>
          <spirit:size>32</spirit:size>
          <spirit:field>
            <spirit:name>IN</spirit:name>
            <spirit:bitRange>[15:0]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Input data</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>OUT</spirit:name>
            <spirit:bitRange>[31:16]</spirit:bitRange>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Output data</spirit:description>
            <spirit:volatile>true</spirit:volatile>
          </spirit:field>
        </spirit:register>
      </spirit:addressBlock>
    </spirit:memoryMap>
  </spirit:memoryMaps>
</spirit:component>